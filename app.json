{
  "expo": {
    "name": "Pinnacle",
    "slug": "buildathon",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "FitForm uses the camera for form feedback."
      }
    },
    "android": {
      "permissions": ["CAMERA"]
    },
    "extra": {
      "eas": {
        "projectId": "c5c8ea7d-4dc6-4465-ab25-c26c750436d6"
      }
    }
  }
}
